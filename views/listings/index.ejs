<% layout("layouts/boilerplate") %>
<body>
    
            <h2>All Listings</h2>
    
    <div class="row row-cols-lg-3 row-cols-md-3 row-cols-sm-1 row-cols-1 ">
        <% listings.forEach(listing => { %>
            <a href="/listings/<%= listing._id%>" class="text-decoration-none text-dark">
            <div class="card col">
                <img src="<%= listing.image.url %>" class="card-img-top" alt="..." style="height: 20rem;">
                <div class="card-img-overlay"><%= listing.title %></div>
                <div class="card-body mt-1">
                <h5 class="card-title"><b><%= listing.title %></b></h5>
                <p class="card-text"> <%= listing.description %></p>
                 </div>
            </div>
            </a>
        <% }) %>
    </div>>
    
    
</body>